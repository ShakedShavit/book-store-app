geRepeatPasswordInput}\r\n            onFocus={onFocusRepeatPasswordInput}\r\n            onBlur={onBlurRepeatPasswordInput}\r\n        />\r\n        {passwordInput !== \"\" && !isRepeatPasswordInputValid && !isInsideRepeatPasswordInput && <span className=\"invalid-message\">Repeated password does not match password or is empty</span>}\r\n        \r\n        <button type=\"submit\" disabled={isFormValid}>Sign Up</button>\r\n\r\n        {hasLoginFailed && <span className=\"invalid-message\">Sign up failed. Please try again</span>}\r\n\r\n        <NavLink to=\"/login\">Login</NavLink>\r\n    </form>\r\n  );\r\n}\r\n\r\nexport default SignupForm;","D:\\Shaked\\Talpiot-HyperActive\\fullstack apps\\book-store-app\\client\\src\\actions\\loginActions.js",[],["136","137"],"D:\\Shaked\\Talpiot-HyperActive\\fullstack apps\\book-store-app\\client\\src\\server\\db\\user.js",[],"D:\\Shaked\\Talpiot-HyperActive\\fullstack apps\\book-store-app\\client\\src\\cookies\\userDataCookies.js",[],"D:\\Shaked\\Talpiot-HyperActive\\fullstack apps\\book-store-app\\client\\src\\server\\db\\book.js",[],["138","139"],"D:\\Shaked\\Talpiot-HyperActive\\fullstack apps\\book-store-app\\client\\src\\cookies\\booksDataCookies.js",[],"D:\\Shaked\\Talpiot-HyperActive\\fullstack apps\\book-store-app\\client\\src\\actions\\booksActions.js",[],"D:\\Shaked\\Talpiot-HyperActive\\fullstack apps\\book-store-app\\client\\src\\components\\shoppingCart\\ShoppingCart.js",[],"D:\\Shaked\\Talpiot-HyperActive\\fullstack apps\\book-store-app\\client\\src\\components\\shoppingCart\\CartItem.js",["140"],"D:\\Shaked\\Talpiot-HyperActive\\fullstack apps\\book-store-app\\client\\src\\actions\\userCartActions.js",[],"D:\\Shaked\\Talpiot-HyperActive\\fullstack apps\\book-store-app\\client\\src\\components\\books\\GoToCheckoutModal.js",["141"],"import React from 'react';\r\nimport { useHistory } from 'react-router-dom';\r\n\r\nconst GoToCheckoutModal = (props) => {\r\n    const closeModalButton = (e) => {\r\n        closeMod